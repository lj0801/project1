<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">
    <bwei.com.day7_rikao.View_Action.TopBars
        android:id="@+id/top_bar_ben"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:leftBackground="@drawable/ic_keyboard_backspace_black_24dp"
        app:rightBackground="@drawable/ic_menu_black_24dp"
        app:titleText="网络"
        app:titleTextSize="15sp"
        app:titleTextColor="@color/colorAccent"
        >
    </bwei.com.day7_rikao.View_Action.TopBars>
    <com.jcodecraeer.xrecyclerview.XRecyclerView
        android:id="@+id/quan_xrecycler"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </com.jcodecraeer.xrecyclerview.XRecyclerView>
</LinearLayout>
